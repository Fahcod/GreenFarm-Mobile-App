{
  "expo": {
    "name": "GreenFarm",
    "slug": "GreenFarm",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/app_assets/icon.png",
    "scheme": "greenfarm",
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/c4d5769a-de16-45b1-9f36-bd8f1c781c14"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/app_assets/icon.png",
        "backgroundImage": "./assets/app_assets/icon.png",
        "monochromeImage": "./assets/app_assets/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fahcodcode.GreenFarm"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/app_assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/app_assets/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ],
      [
       "expo-font",
      {
        "fonts":[
          "./assets/fonts/Poppins-Bold.ttf",
          "./assets/fonts/Poppins-ExtraBold.ttf",
          "./assets/fonts/Poppins-Light.ttf",
          "./assets/fonts/Poppins-Medium.ttf",
          "./assets/fonts/Poppins-Regular.ttf",
          "./assets/fonts/Poppins-SemiBold.ttf"
        ]
      }
      ],
      "expo-video",
      "expo-sqlite",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c4d5769a-de16-45b1-9f36-bd8f1c781c14"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
